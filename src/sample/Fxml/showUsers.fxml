<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="730.0" prefWidth="900.0" style="-fx-background-color: #2f4f4f;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ShowUsersInterface">
   <children>
      <TableView fx:id="tableUsers" prefHeight="615.0" prefWidth="900.0">
        <columns>
          <TableColumn fx:id="columnSurname" prefWidth="300.0" text="Фамилия" />
          <TableColumn fx:id="columnName" minWidth="0.0" prefWidth="300.0" text="Имя" />
            <TableColumn fx:id="columnLogin" prefWidth="300.0" text="Логин" />
        </columns>
      </TableView>
      <Button fx:id="buttonDelete" layoutX="26.0" layoutY="637.0" mnemonicParsing="false" onAction="#deleteRow" prefHeight="26.0" prefWidth="220.0" style="-fx-background-color: #F0FFF0;" text="Удалить" textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <Button fx:id="buttonSave" layoutX="340.0" layoutY="637.0" mnemonicParsing="false" onAction="#save" prefHeight="26.0" prefWidth="220.0" style="-fx-background-color: #F0FFF0;" text="Сохранить" textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <Button fx:id="buttonExitShowUsers" layoutX="645.0" layoutY="637.0" mnemonicParsing="false" onAction="#exitShowUsers" prefHeight="26.0" prefWidth="220.0" style="-fx-background-color: #F0FFF0;" text="Вернуться назад" textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
